<!-- 可用组件：https://mp.weixin.qq.com/debug/wxadoc/dev/component/ -->
<view class="home_page_box">
  <cover-image class="home_banner_box" src="../../images/home/banner.png"></cover-image>
  <view class="home_handle_box">
    <view wx:for="{{viewData}}" wx:key="index" class="home_handle_item">
      <view class="item_body">
        <text class="item_title">{{item.title}}</text>
        <text class="item_subtitle">{{item.subtitle}}</text>
      </view>
      <cover-image class="item_ico_image" :src="{{item.url}}"></cover-image>
    </view>
  </view>
</view>